{
  "name": "powerapi",
  "version": "0.1.0",
  "description": "powershell web api connectivity",
  "main": "server\\server-prod.js",
  "scripts": {
    "dev-server": "webpack-dev-server --config webpack-dev-server.config.js --progress --colors --port 2992 --inline",
    "hot-dev-server": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 2992 --inline",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors",
    "start-dev": "node server/server-dev",
    "start": "node server/server-prod"
  },
  "author": "Cole Chamberlain",
  "repository": "https://github.com/cchamberlain/powerapi",
  "license": "MIT",
  "babel": {
    "stage": 0,
    "env": {
      "development": {
        "plugins": [
          "react-transform"
        ],
        "extra": {
          "react-transform": {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": [
                  "react"
                ],
                "locals": [
                  "module"
                ]
              }
            ]
          }
        }
      }
    }
  },
  "dependencies": {
    "babel": "^5.8.23",
    "babel-loader": "latest",
    "bunyan": "^1.5.1",
    "css-loader": "^0.19.0",
    "items-store": "^0.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "react": "^0.14.0",
    "react-bootstrap": "^0.27.1",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.0-rc3",
    "react-transform-hmr": "^1.0.1",
    "restify": "^4.0.3",
    "style-loader": "^0.12.4",
    "superagent": "^1.4.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "latest",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "bootstrap": "^3.3.5",
    "bootstrap-webpack": "0.0.5",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "font-awesome": "^4.4.0",
    "font-awesome-webpack": "0.0.4",
    "imports-loader": "^0.6.4",
    "jquery": "^2.1.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
